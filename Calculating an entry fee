print("Welcome message")
correct = False
price = 10

while not correct:
    Email = input("Enter email address >>> ")
    Skill = input("Enter your skill level : E for expert and C for casual >>> ")
    country = input("Enter Country UK/US/AU \n").upper
    if Skill == "E":
        price += 35
        correct = True
    else:
        price += 20
        correct = True
    if country == "US":
        price *= 1.5
    elif country == "AU":
        price *= 2

        break
print(price)
another = input("Do you want to enter another fee? y or n").lower()

if another == "n":
    correct = True
else:
    correct == False
